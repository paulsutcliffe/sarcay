    = stylesheet_link_tag "application", :media => "all"
    = favicon_link_tag '/ico/sarcay-icon-144.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144'
    = favicon_link_tag '/ico/sarcay-icon-114.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '114x114'
    = favicon_link_tag '/ico/sarcay-icon-72-precomposed.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '72x72'
    = favicon_link_tag '/ico/sarcay-icon-57.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png'
    = favicon_link_tag '/ico/favicon.ico', :rel => 'shortcut icon'

    = stylesheet_link_tag "application"

    = yield(:head)
